<% content_for :page_title do %>Create Meeting<% end %>
<% content_for :header_data do %>
    <script type="text/javascript">
      $(function() {
        $("#meeting_meeting_date").datepicker();
      });
    </script>
<% end %>

<% form_for :meeting, :url => meetings_path do |f| %>
  <%= render :partial => "meeting_details", :object => f %>
  <%= submit_tag 'Create' %>
<% end %>
<p><%= link_to "Return to the list of meetings", meetings_path %></p>
